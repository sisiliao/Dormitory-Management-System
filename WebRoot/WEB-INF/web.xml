<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BuildingServlet</servlet-name>
    <servlet-class>servlet.BuildingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DormServlet</servlet-name>
    <servlet-class>servlet.DormServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>StuServlet</servlet-name>
    <servlet-class>servlet.StuServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BuildingServlet</servlet-name>
    <url-pattern>/BuildingServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DormServlet</servlet-name>
    <url-pattern>/DormServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StuServlet</servlet-name>
    <url-pattern>/StuServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
